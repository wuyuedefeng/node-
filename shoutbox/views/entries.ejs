<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<% include menu %>
<h3><%= title %></h3>
<% entries.forEach(function(entry){ %>
    <div class="entry">
        <h3><%= entry.title %></h3>
        <p><%= entry.body %></p>
        <p></p>
    </div>
<% }) %>
<p><%= JSON.stringify(entries) %></p>
</body>
</html>