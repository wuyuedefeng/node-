<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title><%= title %>></title>
</head>
<body>
    <h1>PHOTOS</h1>
    <p>Express stock photo application.</p>
    <div class="photos">
        <% photos.forEach(function(photo){ %>
           <div class="photo">
               <h2><%= photo.name %></h2>
               <img src="<%=photo.path%>">
           </div>
        <% }) %>
    </div>
</body>
</html>